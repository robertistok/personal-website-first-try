{
  "compilerOptions": {
    "noEmit": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strict": false,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "jsx": "preserve",
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node"
  },
  "include": ["src", "declaration.d.ts"]
}
